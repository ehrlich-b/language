// Expected: 0
// Factorial - tests loops and arithmetic
func factorial(n i64) i64 {
    var result i64 = 1;
    var i i64 = 1;
    while i <= n {
        result = result * i;
        i = i + 1;
    }
    return result;
}

func main() i64 {
    var x i64 = factorial(5);
    // x should be 120
    if x == 120 {
        syscall(1, 1, "ok\n", 3);
    } else {
        syscall(1, 1, "fail\n", 5);
    }
    return 0;
}
