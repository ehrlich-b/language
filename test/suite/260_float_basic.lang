    var result i64 = 0;

    // fadd: 1.5 + 2.5 = 4.0
    if 1.5 + 2.5 > 3.9 {
        result = result + 10;
    }

    // fsub: 5.5 - 2.5 = 3.0
    if 5.5 - 2.5 > 2.9 {
        result = result + 20;
    }

    // fmul: 2.5 * 4.0 = 10.0
    if 2.5 * 4.0 > 9.9 {
        result = result + 30;
    }

    // fdiv: 10.0 / 4.0 = 2.5
    if 10.0 / 4.0 > 2.4 {
        result = result + 40;
    }

    // fcmp oeq (==)
    if 3.0 == 3.0 {
        result = result + 1;
    }

    // fcmp one (!=)
    if 3.0 != 4.0 {
        result = result + 2;
    }

    // fcmp olt (<)
    if 2.5 < 3.5 {
