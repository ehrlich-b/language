    if 3.5 <= 3.5 {
        result = result + 4;
    }

    // fcmp oge (>=)
    if 4.5 >= 4.5 {
        result = result + 5;
    }

    // All pass: 10+20+30+40+1+2+3+4+5 = 115
    return result;
}
