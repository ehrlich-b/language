// Expected: 0
func main() i64 {
    // Test bitwise AND: 5 & 3 = 1
    var a i64 = 5 & 3;
    if a != 1 { return 1; }

    // Test bitwise OR: 5 | 3 = 7
    var b i64 = 5 | 3;
    if b != 7 { return 2; }

    // Test bitwise XOR: 5 ^ 3 = 6
    var c i64 = 5 ^ 3;
    if c != 6 { return 3; }

    // Test left shift: 1 << 4 = 16
    var d i64 = 1 << 4;
    if d != 16 { return 4; }

    // Test right shift: 16 >> 2 = 4
    var e i64 = 16 >> 2;
    if e != 4 { return 5; }

    // Test combined: (1 << 3) | (1 << 1) = 10
    var f i64 = (1 << 3) | (1 << 1);
    if f != 10 { return 6; }

    // Test precedence: 3 | 4 & 6 should be 3 | (4 & 6) = 3 | 4 = 7
    // Because & has higher precedence than |
    var g i64 = 3 | 4 & 6;
    if g != 7 { return 7; }

    return 0;
}
