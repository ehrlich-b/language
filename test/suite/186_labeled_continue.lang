// Expected: 15
// Test labeled continue - continues outer loop from inner loop
func main() i64 {
    var sum i64 = 0;
    var i i64 = 0;

    outer:
    while i < 3 {
        var j i64 = 0;
        while j < 10 {
            sum = sum + 1;
            if j == 4 {
                i = i + 1;
                continue outer;  // skip rest of inner, continue outer
            }
            j = j + 1;
        }
        i = i + 1;
    }

    // 3 outer iterations * 5 inner increments = 15
    return sum;
}
